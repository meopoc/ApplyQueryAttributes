<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="ApplyQueryAttributes.Views.PageA"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
    xmlns:viewModel="clr-namespace:ApplyQueryAttributes.ViewModels"
    Title="Page A"
    Shell.BackgroundColor="#36424a"
    Shell.TitleColor="White">

    <VerticalStackLayout Padding="10,10,10,10">

        <Button
            Command="{Binding NavigateCommand}"
            CommandParameter="PageB"
            Text="Navigate to Page B" />

    </VerticalStackLayout>

</ContentPage>