<ContentView
    x:Class="ApplyQueryAttributes.Views.FlyoutMenu"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <StackLayout>

        <Label Text="{Binding Name}" />

        <Button
            Command="{Binding NavigateCommand}"
            CommandParameter="MainPage"
            Text="Main Page No Menu" />

        <Button
            Command="{Binding NavigateCommand}"
            CommandParameter="PageWithFlyoutMenu_1"
            Text="PageWithFlyoutMenu_1" />

        <Button
            Command="{Binding NavigateCommand}"
            CommandParameter="PageWithFlyoutMenu_2"
            Text="PageWithFlyoutMenu_2" />

    </StackLayout>

</ContentView>

